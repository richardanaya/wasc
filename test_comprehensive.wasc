// Test wasc compiler features

memory 1;
table funcref 10;

// Import external JS function
extern externref console_log(externref msg);
extern i32 js_add(i32 a, i32 b);

// Function using references
export i32 test_refs() {
    externref null_ref = ref.null externref;
    i32 is_null = ref.is_null(null_ref);
    
    if (is_null) {
        return 1;
    }
    
    return 0;
}

// Function using arithmetic
export i32 add(i32 a, i32 b) {
    return a + b;
}

// Main entry point
export i32 main() {
    i32 x = 10;
    i32 y = 32;
    i32 result = add(x, y);
    return result;
}
